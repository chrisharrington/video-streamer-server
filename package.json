{
  "name": "video-streamer-server",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "typecheck": "./node_modules/.bin/tsc",
    "api": "./node_modules/.bin/tsc && node api/app.js",
    "api:dev": "./node_modules/.bin/nodemon --inspect=0.0.0.0:9000 --watch './**/*.js' api/app.js",
    "api:logs": "docker-compose logs -f --tail 100 api",
    "indexer": "./node_modules/.bin/tsc && node indexer/app.js",
    "indexer:dev": "./node_modules/.bin/nodemon --watch './**/*.ts' --exec './node_modules/.bin/ts-node' indexer/app.ts",
    "indexer:logs": "docker-compose logs -f --tail 100 indexer",
    "converter": "./node_modules/.bin/tsc && node converter/app.js",
    "converter:dev": "./node_modules/.bin/nodemon --watch './**/*.ts' --exec './node_modules/.bin/ts-node' converter/app.ts",
    "converter:logs": "docker-compose logs -f --tail 100 converter",
    "metadata": "./node_modules/.bin/tsc && node metadata/app.js",
    "metadata:dev": "./node_modules/.bin/nodemon --watch './**/*.ts' --exec './node_modules/.bin/ts-node' metadata/app.ts",
    "metadata:logs": "docker-compose logs -f --tail 100 metadata",
    "subtitler": "./node_modules/.bin/tsc && node subtitler/app.js",
    "subtitler:dev": "./node_modules/.bin/nodemon --watch './**/*.ts' --exec './node_modules/.bin/ts-node' subtitler/app.ts",
    "subtitler:logs": "docker-compose logs -f --tail 100 subtitler"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/chrisharrington/video-streamer-server.git"
  },
  "author": "Chris Harrington",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/chrisharrington/video-streamer-server/issues"
  },
  "homepage": "https://github.com/chrisharrington/video-streamer-server#readme",
  "_moduleAliases": {
    "@root": "./",
    "@lib": "./lib/",
    "@api": "./api/",
    "@indexer": "./indexer/",
    "@converter": "./converter/",
    "@metadata": "./metadata/",
    "@subtitler": "./subtitler/"
  },
  "dependencies": {
    "actions-on-google": "^2.12.0",
    "amqplib": "^0.5.5",
    "body-parser": "^1.19.0",
    "castv2": "^0.1.10",
    "castv2-client": "^1.2.0",
    "chokidar": "^3.3.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dayjs": "^1.8.18",
    "dialogflow-fulfillment": "^0.6.1",
    "express": "^4.16.3",
    "ffprobe": "^1.1.0",
    "fluent-ffmpeg": "^2.1.2",
    "get-video-duration": "^3.0.0-beta.1",
    "history": "^4.10.1",
    "install": "^0.13.0",
    "mdns": "^2.5.1",
    "module-alias": "^2.1.0",
    "mongodb": "^3.3.3",
    "node-fetch": "^2.6.0",
    "node-watch": "^0.6.3",
    "nodecastor": "^0.1.9",
    "npm": "^6.14.6",
    "opensubtitles-api": "^5.1.0",
    "react": "^16.12.0",
    "recursive-readdir": "^2.2.2",
    "sharp": "^0.25.2",
    "srt-to-vtt": "^1.1.3",
    "stream-transcoder": "^0.0.5",
    "string-similarity": "^4.0.1",
    "unique-string": "^2.0.0",
    "uuid": "^8.1.0",
    "zlib": "^1.0.5"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.8",
    "@types/express": "^4.16.0",
    "@types/fluent-ffmpeg": "^2.1.14",
    "@types/mongodb": "^3.1.4",
    "@types/node": "^14.0.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.2.0",
    "nodemon": "^2.0.4",
    "ts-loader": "^7.0.4",
    "ts-node-dev": "^1.0.0-pre.44",
    "tsc-watch": "^4.2.7",
    "typescript": "3.9.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  }
}
